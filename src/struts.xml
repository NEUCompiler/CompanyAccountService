<struts>
	<package name="companyAccountService" extends="struts-default" namespace="/CompanyAccountService">
		<action name="*_*" class="com.ob.action.{1}Action" method="{2}">
			<result name="loginFailed">/companyAccountService/companyClientLogin.jsp</result>
			<result name="loginSuccess">/companyAccountService/index.jsp</result>
		</action>
	</package>
</struts>    
